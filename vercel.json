{
  "version": 2,
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/data",
      "destination": "/api/data.ts"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
